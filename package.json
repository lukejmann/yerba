{
	"private": true,
	"scripts": {
		"gen": "pnpm gen:prisma && pnpm codegen",
		"gen:prisma": "cd server && cargo prisma generate",
		"build": "turbo run build",
		"codegen": "cargo test -p server api::tests::test_and_export_rspc_bindings -- --exact",
		"app": "pnpm --filter app -- ",
