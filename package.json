		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"prettier": "^2.8.7",
		"turbo": "^1.9.9",
		"turbo-ignore": "^0.3.0",
		"typescript": "^4.9.4",
		"vite": "^4.3.8"
	},
	"overrides": {
		"@types/node": "^18.0.0"
	},
	"engines": {
		"pnpm": ">=8.0.0",
		"npm": "pnpm",
		"yarn": "pnpm",
		"node": ">=18.0.0"
	},
	"eslintConfig": {
